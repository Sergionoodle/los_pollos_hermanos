{# templates/base.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    <link rel="stylesheet" href="{{ asset('css/app.css') }}">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">    <link rel="icon" type="image/x-icon" href="{{ asset("images/lph-logos.png") }}">

</head>
<body>
<div class="full-width-container">
        <div class="header__hero" style="background-image: url('{{ asset('images/header-home.jpg') }}');">
            <a href="/"><img src="{{ asset('images/lph-logos.png') }}" alt="Los Pollos Hermanos"></a>
        </div>
        <ul class="header__nav">
            <div class="container">
                <li><a href="{{ path("app_home") }}">Our Menu</a></li>
                <li><a href="{{ path("app_culture") }}">Our Culture &amp; Values</a></li>
                <li><a href="">Contact us</a></li>
            </div>
        </ul>

    <main>
        {% block body %}{% endblock %}
    </main>

    <footer>
        <p>Â© 2024 Mi sitio web. Todos los derechos reservados.</p>
    </footer>

</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>
